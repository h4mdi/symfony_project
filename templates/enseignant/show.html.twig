

{% block title %}Enseignant{% endblock %}

{% block body %}
    <h1>Enseignant</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ enseignant.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ enseignant.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ enseignant.prenom }}</td>
            </tr>
            <tr>
                <th>Datenaiss</th>
                <td>{{ enseignant.datenaiss ? enseignant.datenaiss|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Cin</th>
                <td>{{ enseignant.cin }}</td>
            </tr>
            <tr>
                <th>Grade</th>
                <td>{{ enseignant.grade }}</td>
            </tr>
            <tr>
                <th>Specialite</th>
                <td>{{ enseignant.specialite }}</td>
            </tr>
            <tr>
                <th>Tel</th>
                <td>{{ enseignant.tel }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('enseignant_index') }}">back to list</a>

    <a href="{{ path('enseignant_edit', {'id': enseignant.id}) }}">edit</a>

    {{ include('enseignant/_delete_form.html.twig') }}
{% endblock %}
